@model IEnumerable<TaskManagerProject.Models.Notification>

@{
    ViewBag.Title = "ProjectManagerNotifications";
}

<h2>Project Manager's Notifications</h2>

<<h3>unopened: @Model.Where(n => n.isOpened == false).Count().ToString();</h3>

@foreach (var notification in Model)
{
    @notification.Title
    @notification.Description
}